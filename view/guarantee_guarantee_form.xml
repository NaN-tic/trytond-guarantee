<form string="Guarantee">
    <label name="party"/>
    <field name="party"/>
    <label name="code"/>
    <field name="code"/>
    <label name="document"/>
    <field name="document"/>
    <label name="type"/>
    <field name="type"/>
    <group id="dates" colspan="4" col="6">
        <label name="start_date"/>
        <field name="start_date"/>
        <label name="end_date"/>
        <field name="end_date"/>
        <label name="in_guarantee"/>
        <field name="in_guarantee"/>
    </group>
    <notebook colspan="4">
        <page name="sale_lines" col="2">
            <field name="sale_lines"/>
            <field name="guarantee_sale_lines"/>
        </page>
        <page name="invoice_lines" col="2">
            <field name="invoice_lines"/>
            <field name="guarantee_invoice_lines"/>
        </page>
        <page name="notes">
            <field name="notes" colspan="4"/>
        </page>
    </notebook>
</form>
